        "def clean_up(item):\n",
        "    if '(' in item:\n",
        "        return item[:item.find('(') - 1]\n",
        "\n",
        "    if '[' in item:\n",
        "        return item[:item.find('[')]\n",
        "\n",
        "\n",
        "town_df =  town_df.applymap(clean_up)\n",
        "town_df.head()"
