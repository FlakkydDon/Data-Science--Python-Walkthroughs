    {
      "cell_type": "code",
      "source": [
        "university_towns_dict = {}\n",
        "\n",
        "with open('/content/university_towns.txt', 'r') as file:\n",
        "  items = file.readlines()\n",
        "  states = list(filter(lambda x: '[edit]' in x, items))\n",
        "\n",
        "  for index, state in enumerate(states):\n",
        "    start = items.index(state) + 1\n",
        "    if index == 49: #since 50 states\n",
        "      end = len(items)\n",
        "    else:\n",
        "      end = items.index(states[index + 1])\n",
        "\n",
        "      pairs = map(lambda x: [state, x], items[start:end])\n",
        "      university_towns_dict.update(pairs)\n",
        "\n",
        "  town_df = pd.DataFrame(university_towns_dict.items(), columns = ['State', 'Region_Name'])\n",
        "  display(town_df.head())"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 206
